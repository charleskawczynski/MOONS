issn = {00219991},
journal = {J. Comput. Phys.},
keywords = {Astrophysics,Divergence cleaning,Divergence constraint,Magnetic fields,Magnetohydrodynamics (MHD),Particle methods,Smoothed particle magnetohydrodynamics (SPMHD)},
number = {21},
pages = {7214--7236},
