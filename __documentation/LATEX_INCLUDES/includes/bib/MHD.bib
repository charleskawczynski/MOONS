author = {Son, D T},
doi = {10.1103/PhysRevD.59.063008},
eprint = {hep-ph/9803412},
file = {:C$\backslash$:/Users/Charlie/Dropbox/UCLA/RESEARCH/{\_}{\_}SCIENTIFIC{\_}LITERATURE/MHD/Son - 1999 - Magnetohydrodynamics of the early Universe and the evolution of primordial magnetic fields.pdf:pdf},
issn = {0556-2821},
journal = {Phys. Rev.},
pages = {1--8},
primaryClass = {hep-ph},
title = {{Magnetohydrodynamics of the early Universe and the evolution of primordial magnetic fields}},
volume = {59},
