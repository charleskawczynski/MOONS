@article{Potherat2010,
abstract = {Magnetohydrodynamic (MHD) turbulence at low magnetic Reynolds number is experimentally investigated by studying a liquid metal flow in a cubic domain. We focus on the mechanisms that determine whether the flow is quasi-two-dimensional, three-dimensional or in any intermediate state. To this end, forcing is applied by injecting a DC current I through one wall of the cube only, to drive vortices spinning along the magnetic field. Depending on the intensity of the externally applied magnetic field, these vortices extend part or all of the way through the cube. Driving the flow in this way allows us to precisely control not only the forcing intensity but also its dimensionality. A comparison with the theoretical analysis of this configuration singles out the influences of the walls and of the forcing on the flow dimensionality. Flow dimensionality is characterised in several ways. First, we show that when inertia drives three-dimensionality, the velocity near the wall where current is injected scales as U-b similar to I-2/3. Second, we show that when the distance l(z) over which momentum diffuses under the action of the Lorentz force (Sommeria {\&} Moreau, J. Fluid Mech., vol. 118, 1982, pp. 507-518) reaches the channel width h, the velocity near the opposite wall U-t follows a similar law with a correction factor (1-h/l(z)) that measures three-dimensionality. When l(z) {\textless} h, by contrast, the opposite wall has less influence on the flow and U-t similar to I-1/2. The central role played by the ratio l(z)/h is confirmed by experimentally verifying the scaling l(z) similar to N-1/2 put forward by Sommeria {\&} Moreau (N is the interaction parameter) and, finally, the nature of the three-dimensionality involved is further clarified by distinguishing weak and strong three-dimensionalities previously introduced by Klein {\&} Potherat (Phys. Rev. Lett., vol. 104 (3), 2010, 034502). It is found that both types vanish only asymptotically in the limit N --{\textgreater} infinity. This provides evidence that because of the no-slip walls, (i) the transition between quasi-two-dimensional and three-dimensional turbulence does not result from a global instability of the flow, unlike in domains with non-dissipative boundaries (Boeck et al. Phys. Rev. Lett., vol. 101, 2008, 244501), and (ii) it does not occur simultaneously at all scales.},
author = {Poth{\'{e}}rat, Alban and Klein, R},
doi = {10.1017/jfm.2014.620},
file = {:C$\backslash$:/Users/Charlie/Dropbox/UCLA/RESEARCH/{\_}{\_}SCIENTIFIC{\_}LITERATURE/MHD/Poth{\'{e}}rat, Klein - 2010 - Why , how and when MHD turbulence at low Rm becomes three-dimensional.pdf:pdf},
isbn = {0022-1120},
issn = {0022-1120},
journal = {J. Fluid Mech.},
keywords = {duct,effective 2-dimensional model,flows,layers,mhd and electrohydrodynamics,mhd turbulence,numerical simulations,obstacles,transition,transverse magnetic-field},
number = {2014},
pages = {168--205},
title = {{Why, how and when MHD turbulence at low Rm becomes three-dimensional}},
url = {{\textless}Go to ISI{\textgreater}://WOS:000344913600006$\backslash$nhttp://journals.cambridge.org/download.php?file=/FLM/FLM761/S002211201400620Xa.pdf{\&}code=aeb8eb6a31a354821339879b90c07d0c},
volume = {761},
