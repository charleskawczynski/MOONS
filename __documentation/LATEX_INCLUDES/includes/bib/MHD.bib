isbn = {0020-7225},
issn = {00207225},
journal = {Int. J. Eng. Sci.},
month = {apr},
number = {5},
pages = {505--514},
title = {{Heat transfer in laminar and turbulent liquid-metal MHD flows in square ducts with thin conducting or insulating walls}},
url = {{\textless}Go to ISI{\textgreater}://WOS:A1997XD61800005},
volume = {35},
year = {1997}
