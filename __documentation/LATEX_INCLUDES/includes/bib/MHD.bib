@article{Potherat2007,
abstract = {Inspired by the experiment from Moresco and Alboussiere [J. Fluid Mech.504, 167 (Year: 2004)], we study the stability of a flow of liquid metal in a rectangular, electrically insulating duct with a steady homogeneous magnetic field perpendicular to two of the walls. In this configuration, the Lorentz force tends to eliminate the velocity variations in the direction of the magnetic field. This leads to a quasi-two-dimensional base flow with Hartmann boundary layers near the walls perpendicular to the magnetic field, and so-called Shercliff layers in the vicinity of the walls parallel to the field. Also, the Lorentz force tends to strongly oppose the growth of perturbations with a dependence along the magnetic field direction. On these grounds, we represent the flow using the model from Sommeria and Moreau [J. Fluid Mech.118, 507 (Year: 1982)], which essentially consists of two-dimensional (2D) motion equations with a linear friction term accounting for the effect of the Hartmann layers. The simplicity of this quasi-2D model makes it possible to study the stability and transient growth of quasi-two-dimensional perturbations over an extensive range of nondimensional parameters and reach the limit of high magnetic fields. In this asymptotic case, the Reynolds number based on the Shercliff layer thickness {\$}\backslashmathrm{\{}Re{\}}/H{\^{}}{\{}1/2{\}}{\$} becomes the only relevant parameter. Tollmien–Schlichting waves are the most linearly unstable mode as for the Poiseuille flow, but for {\$}H\backslashgeq42{\$}, a second unstable mode, symmetric about the duct axis, appears with a lower growth rate. We find that these layers are linearly unstable for {\$}\backslashmathrm{\{}Re{\}}/H{\^{}}{\{}1/2{\}}\backslashgeq48350{\$} and energetically stable for {\$}\backslashmathrm{\{}Re{\}}/H{\^{}}{\{}1/2{\}}\backslashleq65.32{\$}. Between these two bounds, some nonmodal quasi-two-dimensional perturbations undergo some significant transient growth (between two and seven times more than in the case of a purely 2D Poiseuille flow, and for much more subcritical values of {\$}\backslashmathrm{\{}Re{\}}{\$}). In the limit of a high magnetic field, the maximum gain {\$}G{\_}\backslashmathrm{\{}max{\}}{\$} associated with this transient growth is found to vary as {\$}G{\_}\backslashmathrm{\{}max{\}}\backslashsim(\backslashmathrm{\{}Re{\}}/\backslashmathrm{\{}Re{\}}{\_}c){\^{}}{\{}2/3{\}}{\$} and occur at time {\$}t{\_}{\{}G{\_}\backslashmathrm{\{}max{\}}{\}}\backslashsim(\backslashmathrm{\{}Re{\}}/\backslashmathrm{\{}Re{\}}{\_}c){\^{}}{\{}1/3{\}}{\$} for streamwise wavenumbers of the same order of magnitude as the critical wavenumber for the linear stability.},
author = {Poth{\'{e}}rat, Alban},
doi = {10.1063/1.2747233},
file = {:C$\backslash$:/Users/Charlie/Dropbox/UCLA/RESEARCH/{\_}{\_}SCIENTIFIC{\_}LITERATURE/MHD/Poth{\'{e}}rat - 2007 - Quasi-two-dimensional perturbations in duct flows under transverse magnetic field.pdf:pdf},
issn = {10706631},
journal = {Phys. Fluids},
number = {7},
pages = {074104},
title = {{Quasi-two-dimensional perturbations in duct flows under transverse magnetic field}},
url = {http://scitation.aip.org/content/aip/journal/pof2/19/7/10.1063/1.2747233},
volume = {19},
year = {2007}
}
