issn = {00219991},
journal = {J. Comput. Phys.},
keywords = {Boundary element methods,Hydrodynamic and hydromagnetic problems,Magnetohydrodynamics and electrohydrodynamics},
month = {jul},
number = {2},
pages = {540--554},
title = {{An integro-differential formulation for magnetic induction in bounded domains: Boundary element-finite volume method}},
