journal = {J. Comput. Phys.},
keywords = {Direct-to-steady-state,Fully-implicit,Large-scale parallel,Multilevel preconditioner,Newton-Krylov,Resistive MHD,Stabilized finite element},
month = {oct},
number = {20},
pages = {7649--7671},
