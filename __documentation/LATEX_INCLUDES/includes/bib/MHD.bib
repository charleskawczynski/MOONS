issn = {08941777},
journal = {Exp. Therm. Fluid Sci.},
keywords = {heat transfer,interfacial waves,liquid metal,magnetohydrodynamics,pressure drop,stratified flow,two-phase flow},
month = {jan},
number = {1},
pages = {46--57},
title = {{Characteristics of flow and heat transfer in air-mercury two-phase stratified flow under a vertical magnetic field}},
url = {http://linkinghub.elsevier.com/retrieve/pii/0894177794900728},
volume = {8},
year = {1994}
