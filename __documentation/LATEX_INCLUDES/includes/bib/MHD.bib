@article{Imazio2016,
abstract = {The MHD flow driven by a travelling magnetic field (TMF) in an annular channel is investigated numerically. For sufficiently large magnetic Reynolds number Rm, or if a large enough pressure gradient is externally applied, the system undergoes an instability in which the flow rate in the channel dramatically drops from synchronism with the wave to much smaller velocities. This transition takes the form of a saddle-node bifurcation for the time-averaged quantities. In this first paper, we characterize the bifurcation, and study the stability of the flow as a function of several parameters. We show that the bifurcation of the flow involves a bistability between Poiseuille-like and Hartman-like regimes, and relies on magnetic flux expulsion. Based on this observation, new predictions are made for the occurrence of this stalling instability.},
archivePrefix = {arXiv},
arxivId = {1602.02053},
author = {Imazio, Paola Rodriguez and Gissinger, Christophe},
doi = {10.1063/1.4942448},
