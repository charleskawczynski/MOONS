@article{Deluca1992,
abstract = {The paper investigates the dynamical relaxation of a disturbed X-type $\backslash$nmagnetic neutral point in a periodic geometry, with an ignorable $\backslash$ncoordinate, for an incompressible fluid. It is found that the properties $\backslash$nof the current sheet cannot be understood in terms of steady state $\backslash$nreconnection theory or more recent linear dynamical solutions. $\backslash$nAccordingly, a new scaling law for magnetic reconnection is presented, $\backslash$nconsistent with fast energy dissipation (i.e., the dissipation rate at $\backslash$ncurrent maximum is approximately independent of magnetic diffusivity $\backslash$n(eta)). The flux annihilation rate, however, scales at eta exp 1/4, $\backslash$nfaster than the Sweet-Parker rate of sq rt eta but asymptotically much $\backslash$nslower than the dissipation rate. These results suggest a flux pile-up $\backslash$nregime in which the bulk of the free magnetic energy is released as heat $\backslash$nrather than as kinetic energy of mass motion. The implications of our $\backslash$nresults for reconnection in the solar atmosphere and interior are $\backslash$ndiscussed.},
author = {Deluca, Edward E and Craig, Ian J},
doi = {DOI: 10.1086/171319},
file = {:C$\backslash$:/Users/Charlie/Dropbox/UCLA/RESEARCH/{\_}{\_}SCIENTIFIC{\_}LITERATURE/MHD/Deluca - 1992 - Magnetic Reconnection in Incompressible Fluids.pdf:pdf},
issn = {15384357},
journal = {Astrophys. J.},
keywords = {CURRENT SHEETS,ENERGY DISSIPATION,INCOMPRESSIBLE FLUIDS,MAGNETIC FIELD RECONNECTION,MAGNETIC FLUX,MAGNETOHYDRODYNAMICS,SCALING LAWS,SOLAR ATMOSPHERE,SOLAR FLARES,SOLAR MAGNETIC FIELD,SOLAR PHYSICS},
pages = {679--686},
title = {{Magnetic reconnection in incompressible fluids}},
url = {http://adsabs.harvard.edu/abs/1992ApJ...390..679D},
volume = {390},
year = {1992}
}
@article{Deng2001,
abstract = {Magnetic reconnection has a crucial role in a variety of plasma environments in providing a mechanism for the fast release of stored magnetic energy. During reconnection the plasma forms a 'magnetic nozzle', like the nozzle of a hose, and the rate is controlled by how fast plasma can flow out of the nozzle. But the traditional picture of reconnection has been unable to explain satisfactorily the short timescales associated with the energy release, because the flow is mediated by heavy ions with a slow resultant velocity. Recent theoretical work has suggested that the energy release is instead mediated by electrons in waves called 'whistlers', which move much faster for a given perturbation of the magnetic field because of their smaller mass. Moreover, the whistler velocity and associated plasma velocity both increase as the 'nozzle' becomes narrower. A narrower nozzle therefore no longer reduces the total plasma flow-the outflow is independent of the size of the nozzle. Here we report observations demonstrating that reconnection in the magnetosphere is driven by whistlers, in good agreement with the theoretical predictions.},
author = {Deng, X H and Matsumoto, H},
doi = {10.1038/35069018},
file = {:C$\backslash$:/Users/Charlie/Dropbox/UCLA/RESEARCH/{\_}{\_}SCIENTIFIC{\_}LITERATURE/MHD/Deng, Matsumoto - 2001 - Rapid magnetic reconnection in the Earth's magnetosphere mediated by whistler waves.pdf:pdf},
issn = {0028-0836},
journal = {Nature},
number = {6828},
pages = {557--560},
pmid = {11279487},
title = {{Rapid magnetic reconnection in the Earth's magnetosphere mediated by whistler waves.}},
volume = {410},
year = {2001}
}
@article{Denton2009,
abstract = {A simple approach to energy conserving boundary conditions using exact symmetries is described which is especially useful for numerical simulations using the finite difference method. Each field in the simulation is normally either symmetric (even) or antisymmetric (odd) with respect to the simulation boundary. Another possible boundary condition is an antisymmetric perturbation about a nonzero value. One of the most powerful aspects of this approach is that it can be easily implemented in curvilinear coordinates by making the scale factors of the coordinate transformation symmetric about the boundaries. The method is demonstrated for magnetohydrodynamics (MHD), reduced MHD, and a hybrid code with particle ions and fluid electrons. These boundary conditions yield exact energy conservation in the limit of infinite time and space resolution. Also discussed is the interpretation that the particle charge reverses sign at a conducting boundary with boundary normal perpendicular to the background magnetic field. ?? 2009 Elsevier Inc. All rights reserved.},
author = {Denton, R. E. and Hu, Y.},
doi = {10.1016/j.jcp.2009.03.033},
file = {:C$\backslash$:/Users/Charlie/Dropbox/UCLA/RESEARCH/{\_}{\_}SCIENTIFIC{\_}LITERATURE/MHD/Denton, Hu - 2009 - Symmetry boundary conditions.pdf:pdf},
isbn = {0021-9991},
