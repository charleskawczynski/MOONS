@article{Son1999,
abstract = {We show that the decaying magnetohydrodynamic$\backslash$nturbulence leads to a more rapid growth of the$\backslash$ncorrelation length of a primordial magnetic field than$\backslash$nthat caused by the expansion of the Universe. As an$\backslash$nexample, we consider the magnetic fields created during$\backslash$nthe electroweak phase transition. The expansion of the$\backslash$nuniverse alone would yield a correlation length at the$\backslash$npresent epoch of {\{}1 AU{\}}, whereas we find that the$\backslash$ncorrelation length is likely of order {\{}100 AU{\}}, and$\backslash$ncannot possibly be longer than {\{}10{\^{}}4 AU{\}} for$\backslash$nnon-helical fields. If the primordial field is strongly$\backslash$nhelical, the correlation length can be much larger, but$\backslash$nwe show that even in this case it cannot exceed 100 pc.$\backslash$nAll these estimates make it hard to believe that the$\backslash$nobserved galactic magnetic fields can result from the$\backslash$namplification of seed fields generated at the$\backslash$nelectroweak phase transition by the standard galactic$\backslash$ndynamo.},
