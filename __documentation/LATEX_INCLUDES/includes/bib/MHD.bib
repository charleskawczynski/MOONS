@article{Xu2004,
abstract = {The homogeneous dynamo effect is at the root of cosmic magnetic field$\backslash$ngeneration. With only a very few exceptions, the numerical treatment$\backslash$nof homogeneous dynamos is carried out in the framework of the differential$\backslash$nequation approach. The present paper tries to facilitate the use$\backslash$nof integral equations in dynamo research. Apart from the pedagogical$\backslash$nvalue to illustrate dynamo action within the well-known picture of$\backslash$nthe Biot-Savart law, the integral equation approach has a number$\backslash$nof practical advantages. The first advantage is its proven numerical$\backslash$nrobustness and stability. The second and perhaps most important advantage$\backslash$nis its applicability to dynamos in arbitrary geometries. The third$\backslash$nadvantage is its intimate connection to inverse problems relevant$\backslash$nnot only for dynamos but also for technical applications of magnetohydrodynamics.$\backslash$nThe paper provides the first general formulation and application$\backslash$nof the integral equation approach to time-dependent kinematic dynamos,$\backslash$nwith stationary dynamo sources, in finite domains. The time dependence$\backslash$nis restricted to the magnetic field, whereas the velocity or corresponding$\backslash$nmean-field sources of dynamo action are supposed to be stationary.$\backslash$nFor the spherically symmetric $\alpha${\^{}}2 dynamo model it is shown$\backslash$nhow the general formulation is reduced to a coupled system of two$\backslash$nradial integral equations for the defining scalars of the poloidal$\backslash$nand toroidal field components. The integral equation formulation$\backslash$nfor spherical dynamos with general stationary velocity fields is$\backslash$nalso derived. Two numerical examples—the $\alpha${\^{}}2 dynamo model$\backslash$nwith radially varying $\alpha$ and the Bullard-Gellman model—illustrate$\backslash$nthe equivalence of the approach with the usual differential equation$\backslash$nmethod. The main advantage of the method is exemplified by the treatment$\backslash$nof an $\alpha${\^{}}2 dynamo in rectangular domains.},
archivePrefix = {arXiv},
arxivId = {astro-ph/0306393},
author = {Xu, Mingtian and Stefani, Frank and Gerbeth, Gunter},
