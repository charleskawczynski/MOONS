@article{Smolentsev2013,
abstract = {We review previous studies on instabilities and transitions in magnetohydrodynamic flows in a special context of liquid-metal blanket applications. In the past, possible transitions in blanket flows were mostly attributed to instabilities in the Hartmann layers. More recent studies show, however, that the side layers can experience instabilities at sufficiently lower Reynolds numbers. This suggests that in the blanket flows, the appearance of turbulence can most likely be related to the side rather than Hartmann layers. Various factors that may affect stability in blanket flows have been discussed. In particular, buoyancy forces can result in potentially unstable inflectional velocity profiles. First computational results, illustrating possibility of instabilities and quasi-two-dimensional turbulence in vertical mixed-convection flows heated volumetrically are presented. {\textcopyright} 2013 Elsevier B.V. All rights reserved.},
author = {Smolentsev, Sergey and Vetcha, Naveen and Abdou, Mohamed},
doi = {10.1016/j.fusengdes.2013.04.001},
file = {:C$\backslash$:/Users/Charlie/Dropbox/UCLA/RESEARCH/{\_}{\_}SCIENTIFIC{\_}LITERATURE/MHD//Smolentsev, Vetcha, Abdou - 2013 - Effect of a magnetic field on stability and transitions in liquid breeder flows in a blanket.pdf:pdf},
isbn = {0920-3796},
issn = {09203796},
journal = {Fusion Eng. Des.},
keywords = {Blanket,MHD flow,Mixed convection,Stability and transitions},
month = {oct},
number = {6-8},
pages = {607--610},
publisher = {Elsevier B.V.},
title = {{Effect of a magnetic field on stability and transitions in liquid breeder flows in a blanket}},
url = {http://linkinghub.elsevier.com/retrieve/pii/S0920379613003815},
volume = {88},
year = {2013}
}
