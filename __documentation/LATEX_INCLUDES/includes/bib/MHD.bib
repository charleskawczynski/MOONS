issn = {10902716},
journal = {J. Comput. Phys.},
keywords = {Boundary integral method,Exterior problem,Magnetic Reynolds number,Magnetohydrodynamic turbulence,Magnetohydrodynamics},
pages = {320--339},
publisher = {Elsevier Inc.},
title = {{A hybrid finite difference-boundary element procedure for the simulation of turbulent MHD duct flow at finite magnetic Reynolds number}},
url = {http://linkinghub.elsevier.com/retrieve/pii/S0021999115006695},
volume = {304},
