@article{Pouquet1976,
abstract = {Turbulence and magnetic fields are a common feature of many celestial bodies. To understand the turbulent generation of large-scale magnetic fields, a new nonlinear theory is developed for three-dimensional homogeneous isotropic incompressible MHD turbulence with helicity, i.e., not statistically invariant under plane reflexions. A modification of the eddy-damped quasi-normal Markovian approximation is used, where the eddy-damping rate includes a contribution from Alfven waves. The resulting closed-form equations for the evolution of the kinetic and magnetic energy and helicity spectra are studied both theoretically and numerically for cases with high Reynolds number and unit magnetic Prandtl number. A nonlinear turbulent dynamo is obtained which needs only kinetic energy and helicity injection plus a seed of magnetic field. The study confirms the importance of helicity both in its kinetic and in its magnetic form for generation of large-scale magnetic fields by turbulence.},
author = {Pouquet, Annick and Frisch, U. and L{\'{e}}orat, J. and L{\'{e}}eorat, J.},
doi = {10.1017/S0022112076002140},
file = {:C$\backslash$:/Users/Charlie/Dropbox/UCLA/RESEARCH/{\_}{\_}SCIENTIFIC{\_}LITERATURE/MHD/Pouquet et al. - 1976 - Strong MHD helical turbulence and the nonlinear dynamo effect.pdf:pdf},
issn = {0022-1120},
journal = {J. Fluid Mech.},
number = {02},
pages = {321--354},
title = {{Strong MHD helical turbulence and the nonlinear dynamo effect}},
volume = {77},
year = {1976}
}
