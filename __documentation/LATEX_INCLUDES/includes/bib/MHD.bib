issn = {00219991},
journal = {J. Comput. Phys.},
number = {1},
pages = {17--48},
primaryClass = {astro-ph},
title = {{On the divergence-free condition in Godunov-type schemes for ideal magnetohydrodynamics: The upwind constrained transport method}},
volume = {195},
year = {2004}
}
