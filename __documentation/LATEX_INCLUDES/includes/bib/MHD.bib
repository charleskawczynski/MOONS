isbn = {0031-9201},
issn = {00319201},
journal = {Phys. Earth Planet. Inter.},
keywords = {Geodynamo,Mercury,Numerical dynamos,Variable electrical conductivity},
number = {1-2},
pages = {42--53},
title = {{Effects of a radially varying electrical conductivity on 3D numerical dynamos}},
